import java.util.*;
class AvgNonNeg
{
public static void main(String args[])
{
Scanner sc = new Scanner(System.in);
double n = sc.nextInt();

double count=0;
double sum=0;
int i=1;
while(i<=n)
{
double num = sc.nextInt();
if(num>=0)
{
count++;
sum = sum+num;
}
i++;
}
double avg = (sum/count);
System.out.println("Average of the non-negative integers: "+avg);
}
}
